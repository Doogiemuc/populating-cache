{
  "name": "populating-cache",
  "version": "3.1.0",
  "description": "A lightweight client side cache that can automatically populate DBrefs",
  "license": "MIT",
  "repository": "doogiemuc/populating-cache",
  "main": "src/PopulatingCache.js",
  "author": {
    "name": "Robert (DoogieMuc)",
    "email": "coding@doogie.de",
    "url": "https://github.com/DoogieMuc"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "test": "jest",
    "watch": "jest -- watch",
    "coverage": "npm test -- --coverage",
    "postcoverage": "open-cli coverage/lcov-report/index.html",
    "lint": "eslint .",
    "docs": "documentation readme src --section=API",
    "postdocs": "git add README.md",
    "clean": "rimraf dist",
    "build": "babel src -d dist",
    "prerelease": "npm run lint && npm test && npm run build",
    "release": "standard-version",
    "postpublish": "git push origin main --follow-tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "git add"
    ]
  },
  "keywords": [
    "populating-cache",
    "cache",
    "DBrefs"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/preset-env": "7.12.11",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "documentation": "13.1.0",
    "eslint": "7.16.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-prettier": "3.3.0",
    "husky": "4.3.6",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "open-cli": "6.0.1",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "standard-version": "9.0.0"
  }
}
